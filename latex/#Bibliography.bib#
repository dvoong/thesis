@article{1742-6596-293-1-012052,
  author={Irina Machikhiliyan and the LHCb calorimeter group},
  title={Current status and performance of the LHCb electromagnetic and hadron calorimeters},
  journal={Journal of Physics: Conference Series},
  volume={293},
  number={1},
  pages={012052},
  url={http://stacks.iop.org/1742-6596/293/i=1/a=012052},
  year={2011},
  abstract={As a part of the calorimeter system of the LHCb experiment both the Electromagnetic Calorimeter (ECAL) and the Hadron Calorimeter (HCAL) play a key role in the LHCb trigger system providing it with energetic electron, photon and hadron candidates. The article discusses the designs of both calorimeters and reports their present performance as it was obtained from the analysis of early data collected in the spring of 2010.}
}

