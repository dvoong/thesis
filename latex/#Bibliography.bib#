@article{Aaij:1493820,
      author        = "Aaij, R and Albrecht, J et al",
      title         = "{The LHCb Trigger and its Performance in 2011}",
      journal       = "J. Instrum.",
      number        = "arXiv:1211.3055. CERN-LHCb-DP-2012-004. LHCb-DP-2012-004",
      volume        = "8",
      pages         = "P04022. 31 p",
      month         = "Nov",
